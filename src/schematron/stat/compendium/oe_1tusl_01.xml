  </settings>
  <title>
    <item field="okpo" name="Код предприятия" dic="s_okpo" />
    <item field="name" name="Наименование предприятия" />
    <item field="leader_fio" name="Руководитель организации (ФИО)" />
    <item field="responsible_post" name="Должностное лицо, ответственное за составление формы (должность)" />
    <item field="responsible_fio" name="Должностное лицо, ответственное за составление формы (ФИО)" />
    <item field="phone" name="Контактный телефон" />
    <item field="mail" name="E-mail:" />
  </title>
  <sections>
    <section code="1" name="Раздел 1. Состояние условий труда" nb="1">
      <columns>
        <column code="А" type="B" name="Наименование">
          <default-cell column="А" format="C(250)" inputType="0" />
        </column>
        <column code="Б" type="B" name="№ строки">
          <default-cell column="Б" format="C(5)" inputType="0" />
        </column>
        <column code="3" type="Z" name="Весь персонал на конец отчетного года, человек">
          <default-cell column="3" format="N(15,0)" inputType="2" />
        </column>
        <column code="4" type="Z" name="  из них женщины">
          <default-cell column="4" format="N(15,0)" inputType="2" />
        </column>
        <column code="5" type="Z" name="  из них лица моложе 18 лет">
          <default-cell column="5" format="N(15,0)" inputType="2" />
        </column>
      </columns>
      <rows>
        <row code="01" type="F" name="Списочная численность работников (без находящихся в отпуске по беременности и родам и по уходу за ребенком)" />
        <row code="" type="C" name="Из данных строки 01:" />
        <row code="02" type="F" name="  занятые на работах с вредными и (или) опасными условиями труда" />
        <row code="" type="C" name="Из данных стр. 02: занятые под воздействием  вредных факторов производственной среды:" />
        <row code="03" type="F" name="  химического фактора" />
        <row code="04" type="F" name="  биологического фактора" />
        <row code="05" type="F" name="  аэрозолей преимущественно фиброгенного действия" />
        <row code="06" type="F" name="  шума, ультразвука воздушного, инфразвука" />
        <row code="07" type="F" name="  вибрации (общей и локальной)" />
        <row code="08" type="F" name="  неионизирующего излучения" />
        <row code="09" type="F" name="  ионизирующего излучения" />
        <row code="10" type="F" name="  микроклимата" />
        <row code="11" type="F" name="  световой среды" />
        <row code="" type="C" name=" занятые под воздействием факторов трудового процесса:" />
        <row code="12" type="F" name="  тяжести" />
        <row code="13" type="F" name="  напряженности" />
      </rows>
    </section>
    <section code="2" name="Раздел 2.  Гарантии и компенсации, предоставляемые работникам, занятым на работах с вредными и (или) опасными условиями труда" nb="2">
      <columns>
        <column code="А" type="B" name="Наименование">
          <default-cell column="А" format="C(250)" inputType="0" />
        </column>
        <column code="Б" type="B" name="№ строки">
          <default-cell column="Б" format="C(5)" inputType="0" />
        </column>
        <column code="3" type="Z" name="Весь персонал на конец отчетного года, человек">
          <default-cell column="3" format="N(15,0)" inputType="2" />
        </column>
        <column code="4" type="Z" name="  в том числе женщины">
          <default-cell column="4" format="N(15,0)" inputType="2" />
        </column>
        <column code="5" type="Z" name="Фактические расходы на гарантии,компенсации и средства индивидуальной защиты в отчетном году, тыс.руб.">
          <default-cell column="5" format="N(15,1)" inputType="2" />
        </column>
      </columns>
      <rows>
        <row code="" type="C" name="Списочная численность работников, занятых на работах с вредными и (или) опасными усл.труда, имеющих право на:" />
        <row code="20" type="F" name=" ежегодный дополнительный оплачиваемый  отпуск" />
        <row code="21" type="F" name=" сокращенную продолжительность рабочего времени">
          <cell column="5" format="N(15,1)" inputType="0" />
        </row>
        <row code="22" type="F" name=" лечебно-профилактическое питание" />
        <row code="23" type="F" name=" молоко или другие равноценные пищевые продукты" />
        <row code="24" type="F" name=" оплату труда в повышенном размере" />
        <row code="25" type="F" name=" проведение медицинских осмотров" />
        <row code="" type="C" name=" досрочное назначение страховой пенсии по старости:" />
        <row code="26" type="F" name="   по списку N 1">
          <cell column="5" format="N(15,1)" inputType="0" />
        </row>
        <row code="27" type="F" name="   по списку N 2">
          <cell column="5" format="N(15,1)" inputType="0" />
        </row>
        <row code="28" type="F" name="   прочие пенсии за особые условия труда, пенсии за выслугу лет, установленные законодательством">
          <cell column="5" format="N(15,1)" inputType="0" />
        </row>
        <row code="29" type="F" name="Списочная численность работников, которым установлен хотя бы один вид гарантий и компенсаций, перечисленных в стр. с 20 по 28">
          <cell column="5" format="N(15,1)" inputType="0" />
        </row>
        <row code="30" type="F" name="Списочная численность работников, получающих бесплатно спецодежду, спецобувь и другие средства индивидуальной защиты -всего">
          <cell column="4" format="N(15,0)" inputType="0" />
        </row>
        <row code="" type="C" name="Из данных строки 30" />
        <row code="31" type="F" name="Списочная численность работников, получающих бесплатно спецодежду, спецобувь и другие средства индивидуальной защиты, занятых на работах с вредными и (или) опасными условиями труда">
          <cell column="4" format="N(15,0)" inputType="0" />
        </row>
      </rows>
    </section>
  </sections>
