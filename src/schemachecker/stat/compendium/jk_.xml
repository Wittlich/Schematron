<?xml version="1.0" encoding="utf-8"?>
<metaForm code="609255001004" idp="04" idf="1" shifr="jk" name="26-ЖКХ.СВЕДЕНИЯ О ПРЕДОСТАВЛЕНИИ  ГРАЖДАНАМ СОЦИАЛЬНОЙ ПОДДЕРЖКИ  ПО  ОПЛАТЕ  ЖИЛОГО ПОМЕЩЕНИЯ  И КОММУНАЛЬНЫХ  УСЛУГ" obj="okpo" OKUD="0609255" version="19-02-2019" format-version="1.3">
  <settings>
    <sign>true</sign>
    <validation>always</validation>
    <notEmpty>true</notEmpty>
  </settings>
  <title>
    <item field="okpo" name="Код предприятия" dic="s_okpo" />
    <item field="name" name="Наименование предприятия" />
    <item field="leader_fio" name="Руководитель организации (ФИО)" />
    <item field="responsible_post" name="Должностное лицо, ответственное за составление формы (должность)" />
    <item field="oktmo" name="ОКТМО" />
    <item field="responsible_fio" name="Должностное лицо, ответственное за составление формы (ФИО)" />
    <item field="phone" name="Контактный телефон" />
    <item field="mail" name="E-mail:" />
  </title>
  <sections>
    <section code="1" name="Раздел 1. Численность граждан - получателей социальной поддержки по оплате жилого помещения и коммунальных услуг и объем средств, выделенных на эти цели" nb="1">
      <columns>
        <column code="1" type="B" name="Наименование">
          <default-cell column="1" format="C(250)" inputType="0" />
        </column>
        <column code="2" type="B" name="№ строки">
          <default-cell column="2" format="C(5)" inputType="0" />
        </column>
        <column code="3" type="Z" name="Численность граждан, пользующихся социальной поддержкой - всего, чел.">
          <default-cell column="3" format="N(15,1)" inputType="2" />
        </column>
        <column code="4" type="Z" name=" в том числе носители социальной поддержки по оплате ЖКУ, человек">
          <default-cell column="4" format="N(15,0)" inputType="2" />
        </column>
        <column code="5" type="Z" name="Объем средств, предусмотренных на предоставление социальной поддержки по оплате жилого помещения и коммунальных услуг, тыс.руб.">
          <default-cell column="5" format="N(15,1)" inputType="2" />
        </column>
        <column code="6" type="Z" name="в том числе из: федерального бюджета">
          <default-cell column="6" format="N(15,1)" inputType="2" />
        </column>
        <column code="7" type="Z" name="в том числе из: бюджета субъекта РФ">
          <default-cell column="7" format="N(15,1)" inputType="2" />
        </column>
        <column code="8" type="Z" name="в том числе из: местного бюджета">
          <default-cell column="8" format="N(15,1)" inputType="2" />
        </column>
      </columns>
      <rows>
        <row code="1" type="F" name="Всего (стр. 1=стр. 2+стр. 3+стр. 4)">
          <cell column="4" format="N(18,0)" inputType="2" />
          <cell column="5" format="N(18,1)" inputType="2" />
          <cell column="6" format="N(18,1)" inputType="2" />
          <cell column="8" format="N(18,1)" inputType="2" />
          <cell column="3" format="N(18,0)" inputType="2" />
          <cell column="7" format="N(18,1)" inputType="2" />
        </row>
        <row code="" type="C" name="в том числе категории граждан, меры социальной поддержки которых осуществляются по:">
          <cell column="5" format="N(18,2)" inputType="0" />
          <cell column="8" format="N(18,2)" inputType="0" />
          <cell column="3" format="N(18,2)" inputType="0" />
          <cell column="6" format="N(18,2)" inputType="0" />
          <cell column="7" format="N(18,2)" inputType="0" />
          <cell column="4" format="N(18,2)" inputType="0" />
        </row>
        <row code="2" type="F" name=" обязательствам Российской Федерации">
          <cell column="6" format="N(18,1)" inputType="2" />
          <cell column="5" format="N(18,1)" inputType="2" />
          <cell column="7" format="N(18,1)" inputType="2" />
          <cell column="3" format="N(18,0)" inputType="2" />
          <cell column="4" format="N(18,0)" inputType="2" />
          <cell column="8" format="N(18,1)" inputType="2" />
        </row>
        <row code="3" type="F" name="обязательствам субъектов Российской Федерации">
          <cell column="3" format="N(18,0)" inputType="2" />
          <cell column="7" format="N(18,1)" inputType="2" />
          <cell column="4" format="N(18,0)" inputType="2" />
          <cell column="6" format="N(18,1)" inputType="2" />
          <cell column="8" format="N(18,1)" inputType="2" />
          <cell column="5" format="N(18,1)" inputType="2" />
        </row>
        <row code="4" type="F" name="нормативным правовым актам местных органов самоуправления субъекта Российской Федерации">
          <cell column="4" format="N(18,0)" inputType="2" />
          <cell column="7" format="N(18,1)" inputType="2" />
          <cell column="8" format="N(18,1)" inputType="2" />
          <cell column="3" format="N(18,0)" inputType="2" />
          <cell column="6" format="N(18,1)" inputType="2" />
          <cell column="5" format="N(18,1)" inputType="2" />
        </row>
        <row code="" type="C" name="Справочно:">
          <cell column="3" format="N(18,2)" inputType="0" />
          <cell column="4" format="N(18,2)" inputType="0" />
          <cell column="6" format="N(18,2)" inputType="0" />
          <cell column="5" format="N(18,2)" inputType="0" />
          <cell column="7" format="N(18,2)" inputType="0" />
          <cell column="8" format="N(18,2)" inputType="0" />
        </row>
        <row code="5" type="F" name="Фактически возмещено средств на предоставление социальной поддержки по оплате жилого помещения и коммунальных услуг из бюджетов всех уровней, всего, тыс руб">
          <cell column="3" format="N(18,2)" inputType="2" />
          <cell column="4" format="N(18,2)" inputType="0" />
          <cell column="5" format="N(18,2)" inputType="0" />
          <cell column="6" format="N(18,2)" inputType="0" />
          <cell column="7" format="N(18,2)" inputType="0" />
          <cell column="8" format="N(18,2)" inputType="0" />
        </row>
        <row code="6" type="F" name="в том числе за прошлые периоды, тыс руб ">
          <cell column="3" format="N(18,2)" inputType="2" />
          <cell column="5" format="N(18,2)" inputType="0" />
          <cell column="6" format="N(18,2)" inputType="0" />
          <cell column="7" format="N(18,2)" inputType="0" />
          <cell column="8" format="N(18,2)" inputType="0" />
          <cell column="4" format="N(18,2)" inputType="0" />
        </row>
      </rows>
    </section>
  </sections>
  <controls>
    <control id="5" name="Стр. 01=стр.02+стр.3+стр.4 по графам 3-8" condition="" rule="{[1][1][3-8]}|=|{[1][2][3-8]}+{[1][3][3-8]}+{[1][4][3-8]}" precision="2" tip="1" />
    <control id="6" name="Стр. 01&gt;=стр.02 по графам 3-8" condition="" rule="{[1][1][3-8]}|&gt;=|{[1][2][3-8]}" precision="2" tip="1" />
    <control id="7" name="Стр. 01&gt;=стр.03 по графам 3-8" condition="" rule="{[1][1][3-8]}|&gt;=|{[1][3][3-8]}" precision="2" tip="1" />
    <control id="8" name="гр. 05=гр.06+ гр.07+гр.08 по всем строкам" condition="" rule="{[1][*][5]}|=|{[1][*][6]}+{[1][*][7]}+{[1][*][8]}" precision="2" tip="1" />
    <control id="9" name="Стр. 01&gt;=стр.04 по графам 3-8" condition="" rule="{[1][1][3-8]}|&gt;=|{[1][4][3-8]}" precision="2" tip="1" />
    <control id="11" name="стр. 05&gt;=стр.06 " condition="" rule="{[1][5][3]}|&gt;=|{[1][6][3]}" precision="1" tip="1" />
    <control id="38" name="гр.03&gt;=гр.04 по строкам 1-4" condition="" rule="{[1][1-4][3]}|&gt;=|{[1][1-4][4]}" precision="0" tip="1" />
  </controls>
