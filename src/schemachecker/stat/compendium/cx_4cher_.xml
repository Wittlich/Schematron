  <sections>
    <section code="1" name="Раздел 1. Сведения о проведении мероприятий на землях, загрязненных радионуклидами" nb="1">
      <columns>
        <column code="А" type="B" name="Наименование">
          <default-cell column="А" format="C(250)" inputType="0" />
        </column>
        <column code="Б" type="B" name="№ строки">
          <default-cell column="Б" format="C(5)" inputType="0" />
        </column>
        <column code="3" type="V" name="Единица измерения">
          <default-cell column="3" format="C(10)" inputType="0" />
        </column>
        <column code="4" type="Z" name="Выполнено работ">
          <default-cell column="4" format="N(15,2)" inputType="2" />
        </column>
      </columns>
      <rows>
        <row code="01" type="F" name="Внесено минеральных удобрений на площади">
          <cell column="3" format="C(10)" default="га" inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="02" type="F" name="На эту площадь внесено удобрений в действующем веществе - всего">
          <cell column="3" format="C(10)" default="т" inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="03" type="F" name="Произвестковано кислых почв на площади">
          <cell column="3" format="C(10)" default="га" inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="04" type="F" name="Внесено известковых материалов - всего">
          <cell column="3" format="C(10)" default="т" inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="05" type="F" name="Залужение естественных кормовых угодий">
          <cell column="3" format="C(10)" default="га" inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="06" type="F" name="Перезалужение улучшенных сенокосов и пастбищ">
          <cell column="3" format="C(10)" default="га" inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="07" type="F" name="Осушение земель, включая реконструкцию и восстановление осушительных систем">
          <cell column="3" format="C(10)" default="га" inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
      </rows>
    </section>
    <section code="2" name="Раздел 2. Сведения  о  производстве  продукции  животноводства  и  проведении  мероприятий с  поголовьем  скота  на  территории,  загрязненной   радионуклидами" nb="2">
      <columns>
        <column code="А" type="B" name="Наименование">
          <default-cell column="А" format="C(250)" inputType="0" />
        </column>
        <column code="Б" type="B" name="№ строки">
          <default-cell column="Б" format="C(5)" inputType="0" />
        </column>
        <column code="3" type="V" name="Код по ОКПД">
          <default-cell column="3" format="C(10)" inputType="0" />
        </column>
        <column code="4" type="V" name="Единица измерения">
          <default-cell column="4" format="C(10)" inputType="0" />
        </column>
        <column code="5" type="Z" name="Количество">
          <default-cell column="5" format="N(15,2)" inputType="2" />
        </column>
      </columns>
      <rows>
        <row code="10" type="F" name="Произведено основных видов скота и птицы на убой в живом весе">
          <cell column="4" format="C(10)" default="центнер" inputType="0" />
          <cell column="5" format="N(15,3)" inputType="2" />
        </row>
        <row code="11" type="F" name="Надоено молока крупного рогатого скота">
          <cell column="3" format="C(10)" default="01.21.20" inputType="0" />
          <cell column="4" format="C(10)" default="центнер" inputType="0" />
          <cell column="5" format="N(15,3)" inputType="2" />
        </row>
        <row code="" type="C" name="На конец отчетного года имелось в наличии: " />
        <row code="12" type="F" name="  крупного рогатого скота">
          <cell column="4" format="C(10)" default="голова" inputType="0" />
          <cell column="5" format="N(15,3)" inputType="2" />
        </row>
        <row code="13" type="F" name="    из него коров">
          <cell column="4" format="C(10)" default="голова" inputType="0" />
          <cell column="5" format="N(15,3)" inputType="2" />
        </row>
        <row code="14" type="F" name="  свиней">
          <cell column="4" format="C(10)" default="голова" inputType="0" />
          <cell column="5" format="N(15,3)" inputType="2" />
        </row>
        <row code="15" type="F" name="  овец и коз">
          <cell column="4" format="C(10)" default="голова" inputType="0" />
          <cell column="5" format="N(15,3)" inputType="2" />
        </row>
        <row code="" type="C" name="Произведена замена поголовья, неблагополучного по туберкулезу:">
          <cell column="4" format="C(0)" inputType="0" />
        </row>
        <row code="16" type="F" name="  крупного рогатого скота">
          <cell column="4" format="C(0)" default="голова" inputType="0" />
        </row>
        <row code="17" type="F" name="    из него коров">
          <cell column="4" format="C(0)" default="голова" inputType="0" />
        </row>
        <row code="18" type="F" name="Поступило телок для комплектации вновь отстроенных ферм и для ремонта выбывающего поголовья, заболевшего туберкулезом и лейкозом">
          <cell column="4" format="C(0)" default="голова" inputType="0" />
        </row>
        <row code="19" type="F" name="Исследовано крупного рогатого скота на туберкулез и лейкоз">
          <cell column="4" format="C(1)" default="голова" inputType="0" />
        </row>
        <row code="20" type="F" name="Произведено исследование шерстного покрова овец на уровень радиоактивного заражения">
          <cell column="4" format="C(0)" default="голова" inputType="0" />
        </row>
        <row code="21" type="F" name="Произведена замена доильного оборудования на системы замкнутого типа (АДМ - 8А, АДМ - 8-2, Елочка, Тандем)">
          <cell column="4" format="C(0)" default="единица" inputType="0" />
        </row>
      </rows>
    </section>
