  <sections>
    <section code="1" name="Раздел I. Наличие, движение и состав контрактов, договоров аренды, лицензий, маркетинговых активов и гудвилла (деловой репутации организации)" nb="1">
      <columns>
        <column code="А" type="B" name="Наименование показателей">
          <default-cell column="А" format="C(250)" inputType="0" />
        </column>
        <column code="Б" type="B" name="№ строки">
          <default-cell column="Б" format="C(5)" inputType="0" />
        </column>
        <column code="3" type="Z" name="Наличие по остаточной балансовой стоимости (первоначальной стоимости, с учетом ее изменения за счет амортизации (износа), переоценок и обесценения), на начало года">
          <default-cell column="3" format="N(15,2)" inputType="2" />
        </column>
        <column code="4" type="Z" name="Приобретено за год: на первичном рынке">
          <default-cell column="4" format="N(15,2)" inputType="2" />
        </column>
        <column code="5" type="Z" name="Приобретено за год: на вторичном рынке">
          <default-cell column="5" format="N(15,2)" inputType="2" />
        </column>
        <column code="6" type="Z" name="Выбыло за год за счет: окончания действия или списания вследствие морального износа">
          <default-cell column="6" format="N(15,2)" inputType="2" />
        </column>
        <column code="7" type="Z" name="Выбыло за год за счет: реализации на вторичном рынке - фактическая стоимость реализации">
          <default-cell column="7" format="N(15,2)" inputType="2" />
        </column>
        <column code="8" type="Z" name="Выбыло за год за счет: реализации на вторичном рынке - остаточная балансовая стоимость активов, учтенных в графе 7">
          <default-cell column="8" format="N(15,2)" inputType="2" />
        </column>
        <column code="9" type="Z" name="Начисленная за отчетный год амортизиция (износ), отражаемые в бухгалтерском учете и отчетности">
          <default-cell column="9" format="N(15,2)" inputType="2" />
        </column>
        <column code="10" type="Z" name="Изменение остаточной балансовой стоимости за счет переоценки(+,-) или обесценения (-) по текущей рыночной стоимости">
          <default-cell column="10" format="N(15,2)" inputType="2" />
        </column>
        <column code="11" type="Z" name="Наличие по остаточной балансовой стоимости (первоначальной стоимости, с учетом ее изменения за счет амортизации (износа), переоценок и обесценения), на конец года (сумма граф 3,4,5,10 минус сумма граф 6,8,9)">
          <default-cell column="11" format="N(15,2)" inputType="2" />
        </column>
      </columns>
      <rows>
        <row code="01" type="F" name="Всего экономических активов (сумма строк 02, 07, 08)">
