          <cell column="6" format="N(15,0)" inputType="2" />
          <cell column="7" format="N(15,0)" inputType="2" />
          <cell column="8" format="N(15,0)" inputType="2" />
          <cell column="9" format="N(15,0)" inputType="2" />
          <cell column="10" format="N(15,0)" inputType="2" />
          <cell column="11" format="N(15,0)" inputType="2" />
        </row>
        <row code="08" type="F" name="Приобретенный гудвилл (деловая репутация организации)">
          <cell column="3" format="N(15,0)" inputType="2" />
          <cell column="4" format="N(15,0)" inputType="2" />
          <cell column="5" format="N(15,2)" inputType="0" />
          <cell column="6" format="N(15,2)" inputType="0" />
          <cell column="7" format="N(15,2)" inputType="0" />
